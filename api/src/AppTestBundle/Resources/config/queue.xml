<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="river.queue.delayed_message_producer.in_memory" class="ContinuousPipe\River\Tests\Queue\InMemoryDelayedMessageProducer" />
        <service id="river.queue.delayed_message_producer" alias="river.queue.delayed_message_producer.in_memory" />

        <service id="river.queue.delayed_message_producer.traced"
                 class="ContinuousPipe\River\Tests\Queue\TracedDelayedMessageProducer"
                 decorates="river.queue.delayed_message_producer">
            <argument type="service" id="river.queue.delayed_message_producer.traced.inner" />
        </service>
    </services>
</container>
