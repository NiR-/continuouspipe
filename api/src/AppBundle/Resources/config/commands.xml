<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="app.command.migrate_user_activities" class="AppBundle\Command\MigrateUserActivitiesCommand">
            <argument>app:migrate:user_activities</argument>
            <argument type="service" id="snc_redis.default_client" />
            <argument type="service" id="jms_serializer" />
            <argument type="service" id="app.billing.activity_tracker" />

            <tag name="console.command" />
        </service>
    </services>
</container>