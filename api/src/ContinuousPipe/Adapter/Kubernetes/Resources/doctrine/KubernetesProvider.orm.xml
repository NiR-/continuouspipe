                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          http://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">

    <entity name="ContinuousPipe\Adapter\Kubernetes\KubernetesProvider">
        <id name="identifier" />

        <embedded name="cluster" class="ContinuousPipe\Adapter\Kubernetes\Cluster" />
        <embedded name="user" class="ContinuousPipe\Adapter\Kubernetes\User" />
    </entity>

</doctrine-mapping>
