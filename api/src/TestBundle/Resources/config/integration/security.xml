<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="security.authenticator.client" class="ContinuousPipe\Security\Tests\Authenticator\InMemoryAuthenticatorClient" />
        <service id="security.encryption.vault.previously_known"
                 class="ContinuousPipe\Security\Encryption\InMemory\PreviouslyKnownValuesVault"
                 decorates="security.encryption.vault">
            <argument type="service" id="security.encryption.vault.previously_known.inner" />
        </service>
    </services>
</container>
