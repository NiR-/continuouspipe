        "image": "sroze\/php-example"
      },
      "accessibility": {
        "from_cluster": true,
        "from_external": false
      },
      "scalability": {
        "enabled": true,
        "number_of_replicas": 1
      },
      "port_mappings": [
      ],
      "volumes": [
        {
          "type": "persistent",
          "name": "app-volume",
          "capacity": "5Gi"
        }
      ],
      "volume_mounts": [
        {
          "name": "app-volume",
          "mount_path": "/app/shared"
        }
      ]
    },
    "extensions": [
    ],
    "labels": [
    ],
    "locked": false
  }
]
