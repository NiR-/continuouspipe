{
    "name": "continuouspipe/river",
    "type": "project",
    "description": "ContinuousPipe River",
    "autoload": {
        "psr-4": { "": "src/" },
        "classmap": [ "app/AppKernel.php" ]
    },
    "autoload-dev": {
        "psr-4": { "": "tests/" },
        "files": [ "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php" ]
    },
    "require": {
        "php": ">=5.3.9",
        "symfony/symfony": "~3.4",
        "sensio/framework-extra-bundle": "~3.0,>=3.0.2",
        "sensio/distribution-bundle": "~5.0",
        "doctrine/orm": "~2.5",
        "doctrine/dbal": "~2.5",
        "doctrine/doctrine-bundle": "~1.4",
        "symfony/assetic-bundle": "~2.8",
        "symfony/swiftmailer-bundle": "~3.0",
        "symfony/monolog-bundle": "~3.1",
        "symfony/expression-language": "^3.3.9",
        "incenteev/composer-parameter-handler": "~2.0",
        "csa/guzzle-bundle": "dev-master",
        "friendsofsymfony/rest-bundle": "~1.7",
        "nelmio/cors-bundle": "^1.4",
        "doctrine/doctrine-migrations-bundle": "^1.0",
        "raven/raven": "^0.12.1",
        "sroze/log-stream": "~0.4.5",
        "tolerance/tolerance": "^0.4.0",
        "simple-bus/symfony-bridge": "~5.0",
        "simple-bus/rabbitmq-bundle-bridge": "~4.0.0",
        "simple-bus/jms-serializer-bundle-bridge": "~3.0.0",
        "jms/serializer-bundle": "~2.1.0",
        "mhujer/jms-serializer-uuid-bundle": "~2.0.0",
        "knplabs/github-api": "^1.5",
        "cocur/slugify": "^1.4",
        "keen-io/keen-io": "~2.5",
        "knplabs/knp-paginator-bundle": "^2.5",
        "doctrine/doctrine-cache-bundle": "~1.3.0",
        "rhumsaa/uuid": "^2.8",
        "predis/predis": "^1.1",
        "malkusch/lock": "^1.0",
        "ramsey/uuid-doctrine": "^1.2",
        "guzzlehttp/guzzle": "~6.0",
        "beberlei/metrics": "^2.3",
        "jms/aop-bundle": "^1.2",
        "dbellettini/eventstore-client": "^0.8.0",
        "kreait/firebase-php": "^2.0",
        "adlogix/guzzle-atlassian-connect-middleware": "~1.1.2",
        "twig/twig": "^1.12",
        "google/apiclient": "^2.1.3",
        "lcobucci/jwt": "^3.2",
        "continuous-pipe/events": "dev-master",
        "sroze/kubernetes-client": "dev-add-network-policies",
        "continuous-pipe/message": "dev-master",
        "continuous-pipe/flex": "dev-master",
        "google/cloud": "^0.39.0",
        "docker-php/docker-php": "~1.21.4",
        "react/event-loop": "^0.4.1",
        "elasticsearch/elasticsearch": "^5.1",
        "jamesryanbell/cloudflare": "^1.8",
        "defuse/php-encryption": "^2.0",
        "react/promise": "^2.5",
        "react/promise-timer": "^1.1",
        "facile-it/doctrine-mysql-come-back": "^1.6",
        "behat/symfony2-extension": "2.1.1",
        "lexik/jwt-authentication-bundle": "~1.0",
        "ramsey/uuid": "^3.1",
        "recurly/recurly-client": "^2.7",
        "leafo/scssphp": "^0.6.3",
        "intercom/intercom-php": "^3.0",
        "hwi/oauth-bundle": "^0.5.3",
        "sonata-project/admin-bundle": "^3.1",
        "sonata-project/doctrine-orm-admin-bundle": "^3.0",
        "snc/redis-bundle": "~2.0.2",
        "enqueue/amqp-ext": "^0.8.12"
    },
    "require-dev": {
        "sensio/generator-bundle": "~3.1.6",
        "phpspec/phpspec": "~3.0",
        "behat/behat": "~3.0",
        "phpunit/phpunit": "^5.6",
        "doctrine/data-fixtures": "^1.3",
        "symfony/thanks": "^1.0"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ],
        "update-parameters": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": [{
            "file": "app/config/parameters.yml",
            "env-map": {
                "sentry_dsn": "SENTRY_DSN"
            }
        }]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/continuouspipe/message"
        },
        {
            "type": "vcs",
            "url": "https://github.com/continuouspipe/events"
        },
        {
            "type": "vcs",
            "url": "https://github.com/continuouspipe/flex"
        }
    ]
}
